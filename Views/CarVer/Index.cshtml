@{
    ViewData["Title"] = "车辆核验";
}
<div class="main-content">
    <div class="main-content-inner">
        <div class="page-content">
            <div class="breadcrumbs ace-save-state" id="breadcrumbs">
                <ul class="breadcrumb">
                    <li>
                        <i class="ace-icon fa fa-home home-icon"></i>
                        <a href="/home/index">首页</a>
                    </li>
                    <li>
                        <a href="/carver/index">车辆核验</a>
                    </li>
                </ul><!-- /.breadcrumb -->
            </div>
            <div class="page-header">
                <h1>
                    费用信息<small>
                        <i class="ace-icon fa fa-angle-double-right"></i>
                        入网验证、行驶证验证、车主验证
                    </small>
                </h1>
            </div>
            <div class="row">
                <div class="col-xs-4">
                    <div class="row">
                        <h4>入网验证</h4> 
                    </div>
                    <div class="row">
                        <form class="form-horizontal" action="/gpsuser/save" data-ajax="true" data-ajax-begin="ajax_OnBegin" data-ajax-complete="ajax_OnComplete" data-ajax-failure="failed_resp" data-ajax-method="POST" data-ajax-success="handle_resp" method="post">
                            <div class="form-group">
                                <label class="col-xs-4 control-label " for="form-field-1"> 车辆号码： </label>
                                <div class="col-xs-8">
                                    <input type="text" class="col-xs-10 col-sm-10" name="carcode" placeholder="请输入车牌号码" />
                                </div>
                            </div>
                            <div class="clearfix">
                                <div class="col-md-offset-5 col-md-12">
                                    <button class="btn btn-info" type="submit">
                                        查询
                                    </button>
                                </div>
                            </div>
                        </form>
                    </div>
                </div>
                <div class="col-xs-4">
                    <div class="row">
                        <h4>行驶证验证</h4> 
                    </div>
                    <div class="row">
                        <form class="form-horizontal" action="/gpsuser/save" data-ajax="true" data-ajax-begin="ajax_OnBegin" data-ajax-complete="ajax_OnComplete" data-ajax-failure="failed_resp" data-ajax-method="POST" data-ajax-success="handle_resp" method="post">
                            <div class="form-group">
                                <label class="col-xs-4 control-label " for="form-field-1"> 车辆号码： </label>
                                <div class="col-xs-8">
                                    <input type="text" class="col-xs-10 col-sm-10" name="carcode" placeholder="请输入车牌号码" />
                                </div>
                            </div>
                            <div class="form-group">
                                <label class="col-xs-4 control-label " for="form-field-1"> 车辆颜色： </label>
                                <div class="col-xs-8">
                                    <input type="checkbox" value="黄牌" name="cbocolor" />黄牌
                                    <input type="checkbox" value="蓝牌" name="cbocolor" />蓝牌
                                </div>
                            </div>
                            <div class="clearfix">
                                <div class="col-md-offset-5 col-md-12">
                                    <button class="btn btn-info" type="submit">
                                        查询
                                    </button>
                                </div>
                            </div>
                        </form>
                    </div>
                </div>
                <div class="col-xs-4">
                    <div class="row">
                        <h4>车主验证</h4> 
                    </div>
                    <div class="row">
                        <form class="form-horizontal" action="/gpsuser/save" data-ajax="true" data-ajax-begin="ajax_OnBegin" data-ajax-complete="ajax_OnComplete" data-ajax-failure="failed_resp" data-ajax-method="POST" data-ajax-success="handle_resp" method="post">
                            <div class="form-group">
                                <label class="col-xs-4 control-label " for="form-field-1"> 车辆号码： </label>
                                <div class="col-xs-8">
                                    <input type="text" class="col-xs-10 col-sm-10" name="carcode" placeholder="请输入车牌号码" />
                                </div>
                            </div>
                            <div class="form-group">
                                <label class="col-xs-4 control-label " for="form-field-1"> 车主姓名： </label>
                                <div class="col-xs-8">
                                    <input type="text" class="col-xs-10 col-sm-10" name="carcode" placeholder="请输入车牌号码" />
                                </div>
                            </div>
                            <div class="form-group">
                                <label class="col-xs-4 control-label " for="form-field-1"> 车主电话： </label>
                                <div class="col-xs-8">
                                    <input type="text" class="col-xs-10 col-sm-10" name="carcode" placeholder="请输入车牌号码" />
                                </div>
                            </div>
                            <div class="clearfix">
                                <div class="col-md-offset-5 col-md-12">
                                    <button class="btn btn-info" type="submit">
                                        查询
                                    </button>
                                </div>
                            </div>
                        </form>

                    </div>
                </div>
            </div>
        </div>
    </div>
</div>